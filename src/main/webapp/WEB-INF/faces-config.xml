<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<managed-bean>
		<managed-bean-name>allWs</managed-bean-name>
		<managed-bean-class>com.sai.pumpkin.managedbeans.AllWebservicesController</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>allCon</managed-bean-name>
		<managed-bean-class>com.sai.pumpkin.managedbeans.AllConsumersController</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>allProj</managed-bean-name>
		<managed-bean-class>com.sai.pumpkin.managedbeans.AllProjectsController</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>admin</managed-bean-name>
		<managed-bean-class>com.sai.pumpkin.managedbeans.AdminController</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>release</managed-bean-name>
		<managed-bean-class>com.sai.pumpkin.managedbeans.ReleaseNotesController</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>allOther</managed-bean-name>
		<managed-bean-class>com.sai.pumpkin.managedbeans.AllOtherController</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
        <managed-bean-name>diff</managed-bean-name>
        <managed-bean-class>com.sai.pumpkin.managedbeans.DiffController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>time</managed-bean-name>
        <managed-bean-class>com.sai.pumpkin.managedbeans.ReleaseTimelineController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>allInt</managed-bean-name>
        <managed-bean-class>com.sai.pumpkin.managedbeans.AllInterfaceController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
	<application>
		<action-listener>org.primefaces.application.DialogActionListener</action-listener>
		<navigation-handler>org.primefaces.application.DialogNavigationHandler</navigation-handler>
		<view-handler>org.primefaces.application.DialogViewHandler</view-handler>
	</application>
</faces-config>